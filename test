	TextButton.TextColor3 = Color3.fromRGB(213, 0, 0)
	TextButton.TextSize = 20.000

	-- Scripts:

	local function FFPC_fake_script() -- TextButton.LocalScript 
		local script = Instance.new('LocalScript', TextButton)

		local Frame = script.Parent.Parent

		script.Parent.MouseButton1Click:Connect(function()
			Frame.Visible = false
		end)
	end
	coroutine.wrap(FFPC_fake_script)()
end)
end coroutine.wrap(KMSZU_fake_script)() local function ZBKY_fake_script() -- buttton_4.LocalScript local script = Instance.new('LocalScript', buttton_4)

script.Parent.MouseButton1Click:Connect(function()
	local ReplicatedStorage = game:GetService("ReplicatedStorage")
	local SkinClickEvent = ReplicatedStorage:WaitForChild("Events"):WaitForChild("SkinClickEvent")

	for i = 1, 11 do
		local args = {
			"COW" .. i,
			"v2"
		}
		SkinClickEvent:FireServer(unpack(args))
		task.wait(0.1)
	end
end)
end coroutine.wrap(ZBKY_fake_script)() local function ZXTKKV_fake_script() -- buttton_5.LocalScript local script = Instance.new('LocalScript', buttton_5)

script.Parent.MouseButton1Click:Connect(function()
	-- Anti-AFK infini (exemple générique)
	while true do
		-- Simule une action toutes les 5 minutes
		os.execute("sleep 300") -- 300 secondes = 5 minutes
		-- Simule une touche ou un mouvement (à adapter selon l'environnement)
		print("Anti-AFK : action simulée")
	end

end)


script.Parent.MouseButton1Click:Connect(function()
	game.StarterGui:SetCore("SendNotification", {
		Title = "s0ul Hub",
		Text = "Anti afk Enable",
		Duration = 5,
	})
end)
